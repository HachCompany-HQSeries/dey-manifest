<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default sync-j="4" revision="daisy"/>

  <remote name="digi"  fetch="https://github.com/digidotcom"/>
  <remote name="emb"   fetch="https://github.com/digi-embedded"/>
  <remote name="fsl"   fetch="https://github.com/Freescale"/>
  <remote name="oe"    fetch="https://git.openembedded.org"/>
  <remote name="yocto" fetch="https://git.yoctoproject.org/git"/>

  <project name="meta-digi" path="sources/meta-digi" remote="digi" revision="daisy">
    <copyfile src="sdk/mkproject.sh" dest="mkproject.sh"/>
  </project>
  <project name="meta-fsl-arm"      path="sources/meta-fsl-arm"      remote="yocto" revision="daisy"/>
  <project name="meta-fsl-demos"    path="sources/meta-fsl-demos"    remote="fsl"   revision="daisy"/>
  <project name="meta-openembedded" path="sources/meta-openembedded" remote="oe"    revision="daisy"/>
  <project name="poky"              path="sources/poky"              remote="emb"   revision="daisy"/>
</manifest>
